<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre Marca	</th>
      <th>Direccion	</th>	
      <th>Ciudad	</th>	
      <th>Pais	</th>
      <th>Website	</th>
    </tr>
  </thead>
  <tbody>
  <% @suppliers.each do |supplier| %>
    <tr>
      <td><%= link_to supplier.name, admin_supplier_path(supplier), remote: true 	%>	</td>
      <td><%= supplier.address_1					%>	</td>
      <td><%= supplier.city						%>	</td>
      <td><%= supplier.country					%>	</td>
      <td><%= supplier.website					%>	</td>
      <td><%= link_to "Modificar", edit_admin_supplier_path(supplier), remote: true	%>	</td>
      <td><%= supplier.deleteable? ?  button_to("Eliminar",admin_supplier_path(supplier), method: :delete) : nil %></td>
    </tr>
  <% end %>
  </tbody>
</table>
