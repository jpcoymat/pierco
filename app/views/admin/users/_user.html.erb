<div class="form-group">
  <%= form.label "Primer Nombre", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.text_field :first_name, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Apellido", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.text_field :last_name, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Nombre de Usuario", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.text_field :username, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= form.label "Correo Electronico", class: "col-sm-2 control-label" %>
  <div class="col-xs-4">
    <%= form.text_field :email, class: "form-control" %>
  </div>
</div>
