<h2>Usuarios</h2>

<table class="table table-striped">
<tr><th>Nombre Completo </th> <th>Nombre Usuario </th> <th> Correo</th></tr>
<% @users.each do |user| %>
<tr>
<td><%= user.full_name %></td>
<td><%= user.username %></td>
<td><%= user.email  %> </td>
<td><%= link_to("Editar", edit_admin_user_path(user)) %></td>
<% end %>
</table>

<%= link_to("Creare Nuevo Ususario", new_admin_user_path) %>
